{
  "name": "concierge-service-pro",
  "version": "5.0.0",
  "description": "Женский Консьерж Сервис - Персональный помощник с SMS оплатой",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "prod": "NODE_ENV=production pm2 start server.js --name concierge-service",
    "setup": "npm install && npm run init-db",
    "init-db": "node -e \"require('./db-init.js')\"",
    "backup": "node backup-db.js",
    "migrate": "node db-migrate.js",
    "test-sms": "node test-sms.js",
    "monitor": "node monitor.js",
    "lint": "eslint server.js",
    "clean": "rm -rf node_modules package-lock.json",
    "reset": "npm run clean && npm install && npm run init-db",
    "logs": "pm2 logs concierge-service",
    "status": "pm2 status"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.6",
    "sqlite": "^5.1.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "node-cron": "^3.0.3",
    "moment": "^2.29.4",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.56.0",
    "pm2": "^5.3.0"
  },
  "keywords": [
    "concierge",
    "women",
    "service",
    "assistant",
    "subscription",
    "sms",
    "virtual"
  ],
  "author": "Женский Консьерж",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/concierge-service.git"
  }
}
