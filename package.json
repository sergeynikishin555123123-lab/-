{
  "name": "concierge-app",
  "version": "2.0.0",
  "description": "Женский консьерж-сервис - безопасная версия",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "audit": "npm audit",
    "audit:fix": "npm audit fix --force",
    "security-check": "npm audit --audit-level=critical",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "docker:build": "docker build -t concierge-app .",
    "docker:run": "docker run -p 3000:3000 -e JWT_SECRET=test --env-file .env concierge-app"
  },
  "keywords": ["concierge", "service", "telegram", "timeweb"],
  "author": "Concierge Service",
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2",
    "mongoose": "^8.4.1",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.2",
    "cors": "^2.8.5",
    "xlsx": "^0.18.5",
    "exceljs": "^4.4.0",
    "node-telegram-bot-api": "^0.64.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.13.0",
    "uuid": "^9.0.1",
    "axios": "^1.7.2",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-mongo-sanitize": "^2.2.0",
    "validator": "^13.12.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "validator": "^13.12.0",
    "axios": "^1.7.2",
    "jsonwebtoken": "^9.0.2"
  }
}
