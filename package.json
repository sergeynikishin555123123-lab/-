{
  "name": "concierge-app",
  "version": "4.0.0",
  "description": "Полноценный женский консьерж-сервис",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm ci --only=production --no-audit",
    "audit:fix": "npm audit fix --force",
    "security": "npm audit --audit-level=critical",
    "clean": "rm -rf node_modules package-lock.json",
    "reset": "npm run clean && npm install",
    "docker:build": "docker build -t concierge-app .",
    "docker:run": "docker run -p 3000:3000 --env-file .env concierge-app",
    "test": "node -e \"console.log('✅ Тест пройден')\""
  },
  "keywords": ["concierge", "service", "tasks", "telegram", "admin"],
  "author": "Concierge Service Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.0",
    "mongoose": "^8.7.0",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.2",
    "cors": "^2.8.5",
    "node-telegram-bot-api": "^0.66.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.14.0",
    "exceljs": "^4.4.0",
    "socket.io": "^4.8.0",
    "joi": "^17.13.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.4.0",
    "express-mongo-sanitize": "^2.2.0",
    "validator": "^13.12.0",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.15",
    "axios": "^1.8.2",
    "uuid": "^10.0.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "semver": "^7.6.3",
    "qs": "^6.12.0",
    "tough-cookie": "^4.1.4"
  }
}
